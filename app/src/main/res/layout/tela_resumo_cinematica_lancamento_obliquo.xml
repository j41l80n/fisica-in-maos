<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:background="@color/branco"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none" >
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView android:textColor="@color/preto"
            android:textSize="17sp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_cinematica_13_01"/>
        <ImageView
            android:background="@color/branco"
            android:src="@drawable/iv_0022"
            android:layout_marginBottom="20dp"
            android:layout_width="260dp"
            android:scaleType="fitXY"
            android:layout_gravity="center"
            android:layout_height="200dp" />
        <TextView android:textColor="@color/preto"
            android:textSize="17sp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_cinematica_13_02"/>
        <ImageView
            android:layout_marginTop="10dp"
            android:src="@drawable/iv_lo"
            android:layout_width="100dp"
            android:layout_gravity="center"
            android:layout_height="70dp" />
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_03"/>
                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_01"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text=" m/s"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_001"/>
            </TableRow>
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_04"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_02"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:text=" m/s"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_002"/>
            </TableRow>
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_05"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_03"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_003"/>
            </TableRow>
        </TableLayout>
        <TextView
            android:textSize="17sp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:textColor="@color/branco"
            android:layout_marginBottom="20dp"
            android:id="@+id/tv_tela_resumo_cinematica_13_extra_01"/>
        <TextView android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_cinematica_13_06"/>
        <ImageView
            android:layout_marginTop="10dp"
            android:src="@drawable/iv_lo_2"
            android:layout_width="100dp"
            android:layout_gravity="center"
            android:layout_height="70dp" />
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_07"/>
                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_04"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text=" m/s"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_004"/>
            </TableRow>
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_08"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_05"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:text=" m/s"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_005"/>
            </TableRow>
            <TableRow>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_13_09"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_cinematica_13_06"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView android:textColor="@color/preto"
                    android:textSize="17sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_cinematica_lo_unidades_006"/>
            </TableRow>
        </TableLayout>
        <TextView
            android:textSize="17sp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:textColor="@color/branco"
            android:layout_marginBottom="20dp"
            android:id="@+id/tv_tela_resumo_cinematica_13_extra_02"/>
        <ImageView
            android:src="@drawable/iv_0023"
            android:layout_width="290dp"
            android:scaleType="fitXY"
            android:layout_gravity="center"
            android:layout_marginBottom="40dp"
            android:layout_height="220dp" />
    </LinearLayout>
</ScrollView>