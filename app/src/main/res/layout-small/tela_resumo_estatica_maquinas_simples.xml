<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:background="@color/branco"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none" >
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_01"/>
        <ImageView
            android:layout_marginTop="10dp"
            android:src="@drawable/iv_fl_1"
            android:layout_width="70dp"
            android:layout_gravity="center"
            android:layout_height="20dp" />
        <ImageView
            android:layout_gravity="center"
            android:background="@color/branco"
            android:src="@drawable/iv_0068"
            android:layout_width="250dp"
            android:scaleType="fitXY"
            android:layout_height="170dp" />
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_02"/>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow>
                <TextView android:textSize="17sp"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_03"/>
                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_estatica_maquinas_simples_01"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView
                    android:text="N.m"
                    android:textSize="17sp"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_unidades_0043"/>
            </TableRow>
            <TableRow>
                <TextView android:textSize="17sp"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_04"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_estatica_maquinas_simples_02"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView
                    android:textSize="17sp"
                    android:text="N"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_unidades_0044"/>
            </TableRow>
            <TableRow>
                <TextView android:textSize="17sp"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_05"/>
                <EditText
                    android:ems="4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/et_tela_resumo_estatica_maquinas_simples_03"
                    android:inputType="numberDecimal" >
                </EditText>
                <TextView
                    android:text="m"
                    android:textSize="17sp"
                    android:textColor="@color/preto"
                    android:layout_width="match_parent"
                    android:id="@+id/tv_unidades_0045"/>
            </TableRow>
        </TableLayout>
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginBottom="20dp"
            android:id="@+id/tv_tela_resumo_estatica_3_extra_01"/>
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_06"/>
        <ImageView
            android:layout_marginTop="10dp"
            android:src="@drawable/iv_fl_2"
            android:layout_width="90dp"
            android:layout_gravity="center"
            android:layout_height="40dp" />
        <ImageView
            android:layout_gravity="center"
            android:background="@color/branco"
            android:src="@drawable/iv_0069"
            android:layout_width="250dp"
            android:scaleType="fitXY"
            android:layout_height="120dp" />
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_07"/>
        <ImageView
            android:layout_gravity="center"
            android:background="@color/branco"
            android:src="@drawable/iv_0070"
            android:layout_width="200dp"
            android:scaleType="fitXY"
            android:layout_height="80dp" />
        <TextView android:textSize="17sp"
            android:textColor="@color/preto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tv_tela_resumo_estatica_maquinas_simples_08"/>
        <ImageView
            android:layout_marginTop="10dp"
            android:src="@drawable/iv_fl_3"
            android:layout_marginBottom="50dp"
            android:layout_width="140dp"
            android:layout_gravity="center"
            android:layout_height="60dp" />
    </LinearLayout>
</ScrollView>